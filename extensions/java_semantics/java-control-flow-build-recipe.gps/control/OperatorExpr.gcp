package control;

recipe OperatorExpr(node root, node exit, out node entry) {
	// Recipe for handling all subtypes of the abstract type 'OperatorExpr'.

	choice entry := BinaryExpr(root, exit);
	or entry := ConditionalExpr(root, exit);
	or entry := InstanceOfExpr(root, exit);
	or entry := UnaryExpr(root, exit);
}
