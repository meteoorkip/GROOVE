package control;

recipe ArrayRef(node root, node exit, out node entry) {
	// Match the position and the receiver of the array reference.
	node proot, rroot := rule.ArrayRef(root, exit);
	// Build the flow for receiver.
	node rentry := Ref(rroot, root);
	// Build the flow for position.
	entry := Expr(proot, rentry);
}
